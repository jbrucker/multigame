<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="174.0" prefWidth="349.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.GameController">
   <children>
   <MenuBar VBox.vgrow="NEVER">
        <menus>
          <Menu fx:id="gameMenu" mnemonicParsing="false" text="Game" />
        </menus>
      </MenuBar>
      <Label fx:id="topLabel" text="Waiting for Game">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <Pane fx:id="centerPane" minHeight="-Infinity" prefHeight="108.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Pane>
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER">
         <children>
            <Button fx:id="newGameButton" mnemonicParsing="false" text="New Game" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </FlowPane>
   </children>
</VBox>
